protocol static {
  ipv4;

<%- if config&.static_routes&.v4 -%>
  <%- Array(config&.static_routes&.v4).each do |route| -%>
  route <%= route %>;
  <%- end -%>
<%- end -%>
}

protocol static {
  ipv6;

<%- if config&.static_routes&.v6 -%>
  <%- Array(config&.static_routes&.v6).each do |route| -%>
  route <%= route %>;
  <%- end -%>
<%- end -%>
}

